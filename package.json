{
  "name": "liedjeskaart",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:mobile": "node scripts/dev-mobile.js",
    "update:meta": "node scripts/updateAppHtml.js",
    "build": "npm run update:meta && vite build",
    "build:spa": "npm run update:meta && cross-env BUILD_MODE=spa vite build",
    "preview:spa": "cross-env BUILD_MODE=spa vite build && vite preview",
    "build:static": "npm run update:meta && cross-env BUILD_MODE=static vite build",
    "preview:static": "cross-env BUILD_MODE=static vite build && vite preview",
    "check:routes": "node scripts/checkRoutes.js",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "export:data": "tsx scripts/exportMarkers.ts"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.38.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.47.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22",
    "cross-env": "^10.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "qrcode-terminal": "^0.12.0",
    "svelte": "^5.41.0",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "@googleapis/sheets": "^12.0.0",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.1.0",
    "ol": "^10.6.1"
  }
}
